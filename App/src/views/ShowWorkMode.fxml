<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<BorderPane fx:id="mainPane" style="-fx-background-color: f2b5d4;" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controllers.ShowWorkModeController"
            prefHeight="530.0" prefWidth="800.0">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" style="-fx-background-color: f2b5d4;" VBox.vgrow="ALWAYS">
            <children>
                <Label fx:id="nameOfSubject" layoutX="28.0" layoutY="27.0">
                    <font>
                        <Font name="Segoe Print" size="18.0" />
                    </font>
                </Label>
                <Separator layoutX="28.0" layoutY="58.0" prefHeight="4.0" prefWidth="726.0" style="-fx-background-color: eff7f6;" />
                <TreeView fx:id="apartmentTree" layoutX="28.0" layoutY="70.0" prefHeight="372.0" prefWidth="726.0" style="-fx-background-color: eff7f6;" />
                <Button onAction="#returnToHome" layoutX="690.0" layoutY="460.0" mnemonicParsing="false" style="-fx-background-color: eff7f6;" text="Cancel">
                    <font>
                        <Font name="Segoe Print" size="14.0" />
                    </font>
                </Button>
            </children>
        </AnchorPane>
    </children>

</BorderPane>
